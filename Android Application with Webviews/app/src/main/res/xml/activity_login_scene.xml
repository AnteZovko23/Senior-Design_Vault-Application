<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="73dp"
            android:layout_width="327dp"
            android:id="@+id/login"
            motion:layout_editor_absoluteX="43dp"
            motion:layout_constraintTop_toBottomOf="@+id/signUp"
            android:layout_marginTop="8dp" />
        <Constraint
            motion:layout_editor_absoluteY="171dp"
            android:layout_height="521dp"
            motion:layout_editor_absoluteX="0dp"
            android:layout_width="403dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:layout_height="69dp"
            android:layout_width="326dp"
            android:id="@+id/signUp"
            motion:layout_editor_absoluteY="490dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.517" />
        <Constraint
            motion:layout_constraintVertical_bias="0.971"
            android:layout_height="61dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="73dp"
            motion:layout_constraintTop_toBottomOf="@+id/signUp"
            android:id="@+id/gSignIn"
            motion:layout_editor_absoluteY="657dp"
            motion:layout_editor_absoluteX="169dp" />
        <Constraint
            android:layout_height="63dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.714"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="404dp"
            motion:layout_constraintTop_toBottomOf="@+id/email"
            android:id="@+id/password" />
        <Constraint
            android:layout_height="63dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.428"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="404dp"
            android:id="@+id/email" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>