<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="63dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="404dp"
            motion:layout_constraintTop_toBottomOf="@+id/email"
            android:id="@+id/password" />
        <Constraint
            motion:layout_constraintVertical_bias="0.208"
            android:layout_height="63dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="404dp"
            android:id="@+id/email" />
        <Constraint
            android:layout_height="48dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.426"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="268dp"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:id="@+id/login"
            android:layout_marginTop="68dp"
            motion:layout_constraintBottom_toTopOf="@+id/signUp" />
        <Constraint
            android:layout_height="48dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/gSignIn"
            motion:layout_constraintHorizontal_bias="0.426"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="268dp"
            android:id="@+id/signUp"
            android:layout_marginBottom="220dp" />
        <Constraint
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="119dp"
            android:id="@+id/gSignIn" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>