<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="48dp"
            android:layout_width="268dp"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="80dp"
            android:layout_marginStart="80dp"
            android:id="@+id/login"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="56dp"
            android:layout_marginRight="56dp" />
        <Constraint
            motion:layout_editor_absoluteY="-3dp"
            android:layout_height="279dp"
            android:layout_width="281dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.552" />
        <Constraint
            motion:layout_editor_absoluteY="171dp"
            android:layout_height="521dp"
            motion:layout_editor_absoluteX="0dp"
            android:layout_width="403dp"
            android:id="@+id/imageView5" />
        <Constraint
            motion:layout_constraintVertical_bias="0.784"
            android:layout_height="48dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toTopOf="@+id/gSignIn"
            motion:layout_constraintHorizontal_bias="0.595"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="268dp"
            motion:layout_constraintTop_toBottomOf="@+id/login"
            android:id="@+id/signUp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>