<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment">

    <TextView
        android:id="@+id/text_dashboard"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button9"
        android:layout_width="250dp"
        android:layout_height="106dp"
        android:text="Camera 1"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/text_dashboard"
        app:layout_constraintEnd_toEndOf="@+id/text_dashboard"
        app:layout_constraintStart_toStartOf="@+id/text_dashboard"
        app:layout_constraintTop_toTopOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.192" />

    <Button
        android:id="@+id/button10"
        android:layout_width="250dp"
        android:layout_height="106dp"
        android:text="Camera 3"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/text_dashboard"
        app:layout_constraintEnd_toEndOf="@+id/text_dashboard"
        app:layout_constraintStart_toStartOf="@+id/text_dashboard"
        app:layout_constraintTop_toTopOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.787" />

    <Button
        android:id="@+id/button8"
        android:layout_width="250dp"
        android:layout_height="106dp"
        android:text="Camera 2"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/text_dashboard"
        app:layout_constraintEnd_toEndOf="@+id/text_dashboard"
        app:layout_constraintStart_toStartOf="@+id/text_dashboard"
        app:layout_constraintTop_toTopOf="@+id/text_dashboard"
        app:layout_constraintVertical_bias="0.493" />
</androidx.constraintlayout.widget.ConstraintLayout>