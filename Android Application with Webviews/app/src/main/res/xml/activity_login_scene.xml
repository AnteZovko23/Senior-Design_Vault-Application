<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="133dp"
            android:layout_width="183dp"
            android:id="@+id/login"
            motion:layout_constraintStart_toEndOf="@+id/signUp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            motion:layout_constraintBottom_toTopOf="@+id/gSignIn" />
        <Constraint
            motion:layout_editor_absoluteY="48dp"
            android:layout_height="267dp"
            android:layout_width="374dp"
            android:id="@+id/imageView4"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.486" />
        <Constraint
            motion:layout_editor_absoluteY="171dp"
            android:layout_height="521dp"
            motion:layout_editor_absoluteX="0dp"
            android:layout_width="403dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:layout_height="134dp"
            android:layout_width="183dp"
            android:id="@+id/signUp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/login"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            motion:layout_constraintBottom_toTopOf="@+id/gSignIn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.971"
            android:layout_height="80dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="119dp"
            motion:layout_constraintTop_toBottomOf="@+id/signUp"
            android:id="@+id/gSignIn" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>